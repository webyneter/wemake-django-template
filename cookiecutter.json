{
  "project_name": "test-project",
  "project_verbose_name": "{{ cookiecutter.project_name.replace('-', ' ').title() }}",
  "package_name": "{{ cookiecutter.project_name.replace('-', '_') }}",
  "project_domain": "{{ cookiecutter.project_name.strip().lower().replace('_', '-') | truncate(63 - 12, True) ~ '.example.com' }}",
  "organization": "wemake.services",
  "meta_include": false,
  "meta_output_dir": ".",
  "async": false,
  "line_length": 120,
  "coverage_fail_under": 90,

  "_extensions": ["jinja2_git.GitExtension"],
  "_copy_without_render": [
    ".venv",
    "docs/_build"
  ]
}
